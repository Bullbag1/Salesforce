<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Coupon_consumed</fullName>
    <active>true</active>
    <description>Coupon consumed</description>
    <errorConditionFormula>(quantity__c - Consumed_quantity__c) &lt; 0</errorConditionFormula>
    <errorMessage>Availed coupon limit crossed</errorMessage>
</ValidationRule>
